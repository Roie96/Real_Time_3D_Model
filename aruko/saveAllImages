import numpy as np
import cv2
from matplotlib import pyplot as plt


cap1 = cv2.VideoCapture("C:/Users/talha/RealTimeProject/Real_Time_3D_Model/aruko/low_res.h264")
counter = 0
while counter <= 99:
    ret2, frame2 = cap1.read()

    # Check if frames have been read successfully
    if not ret2:
        print("Error reading video frames.")
        exit()

    if counter % 1 == 0:
        rotated_image1 = cv2.rotate(frame2, cv2.ROTATE_90_CLOCKWISE)
        cv2.imwrite("image"+str(counter)+".jpg", rotated_image1)
    counter += 1